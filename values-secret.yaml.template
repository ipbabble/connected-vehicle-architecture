---
version: "2.0"
# Ideally you NEVER COMMIT THESE VALUES TO GIT (although if all passwords are automatically generated inside
# the vault this should not really matter)

secrets:
  - name: googleapi
    fields:
    - name: key
      value: ..INSERTAPIKEY...
      onMissingValue: error
  - name: weather
    fields:
    - name: openweatherkey
      value: ..APIKEY..
      onMissingValue: error
    - name: ibmweatherkey
      value: ..APIKEY..
      onMissingValue: error
